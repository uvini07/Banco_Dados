SELECT 
		tb_cliente.nome,
		tb_cliente.cnh, 
		
		tb_veiculo.modelo, 
		tb_veiculo.placa, 
		tb_veiculo.disponibilidade, 
		
		tb_aluguel.aluguel_id,
		tb_aluguel.data_inicio,
		tb_aluguel.data_fim

	FROM 
		tb_aluguel
	INNER JOIN 
		tb_cliente ON tb_cliente.cliente_id = tb_aluguel.cliente_id
	INNER JOIN 
		tb_veiculo ON tb_veiculo.veiculo_id = tb_aluguel.veiculo_id;

